{
  "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "sourceMap": true,
      "strict": true,
      "alwaysStrict": true,
      "noImplicitAny": true,
      "strictPropertyInitialization": false,
      "esModuleInterop": true,
      "types": [],
      "experimentalDecorators": true
      //"strictNullChecks": true
  },
  "lib": [
    "es6"
  ],
  "include": [
    "src/*"
  ],
  "exclude": [
    "node_modules",
    "./node_modules",
    "./node_modules/*",
    "./node_modules/@types/node/index.d.ts",
  ]
}
